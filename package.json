{
  "name": "@code0-tech/pictor",
  "version": "0.2.3",
  "description": "A simple template for a custom React component library",
  "scripts": {
    "dev": "npm run storybook:dev",
    "build": "npm run clear && npm run rollup",
    "clear": "concurrently --kill-others \"rimraf ./dist\" \"rimraf ./storybook-static\"",
    "rollup": "rollup -c",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "storybook:test:all": "test-storybook --browsers chromium firefox webkit",
    "storybook:test:update": "npm run storybook:test:all -- -u",
    "docs:preview": "npm run --prefix ../telescopium project:preview pictor"
  },
  "author": "Code0",
  "devDependencies": {
    "@mdx-js/react": "^2.1.2",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@storybook/addon-a11y": "^7.6.4",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/cli": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.3",
    "@tabler/icons-react": "^2.42.0",
    "@types/react": "^18.0.15",
    "axe-playwright": "^1.2.3",
    "babel-loader": "^8.2.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-image-snapshot": "^6.4.0",
    "playwright": "1.44.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "storybook": "^7.5.3",
    "style-loader": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.5",
    "merge-props": "^6.0.0",
    "react-aria": "^3.31.1",
    "react-stately": "^3.29.1",
    "rollup-plugin-visualizer": "^5.11.0"
  }
}
